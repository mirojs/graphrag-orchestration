{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "azureOpenAiApiKey": {
      "value": "${AZURE_OPENAI_API_KEY}"
    },
    "neo4jPassword": {
      "value": "${NEO4J_PASSWORD}"
    },
    "skipRoleAssignments": {
      "value": true
    },
    "enableAuth": {
      "value": false
    },
    "authClientId": {
      "value": "b68b6881-80ba-4cec-b9dd-bd2232ec8817"
    },
    "enableB2C": {
      "value": "${ENABLE_B2C=false}"
    },
    "b2cTenantName": {
      "value": "${B2C_TENANT_NAME}"
    },
    "b2cTenantId": {
      "value": "${B2C_TENANT_ID}"
    },
    "b2cClientId": {
      "value": "${B2C_CLIENT_ID}"
    },
    "b2cClientSecret": {
      "value": "${B2C_CLIENT_SECRET}"
    },
    "serviceGraphragImageName": {
      "value": "${SERVICE_GRAPHRAG_IMAGE_NAME}"
    }
  }
}
